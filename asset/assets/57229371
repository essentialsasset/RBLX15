				<string name="Name">SillyGuitar LS</string>
				<ProtectedString name="Source">-- Silly guitar: because you haven&apos;t lived until you&apos;ve played music on a giant tongue.
math.randomseed(tick()); math.random()

local Tool = script.Parent
local Guitar = Tool.Handle

local myCharacter
local myHumanoid
local Torso
local TorsoWeld

local StrumAnim
local ReadyAnim

local TimeTillEnabled
local WaitScript

local SongSound = Tool:FindFirstChild(&quot;SongSound&quot;)
local waitTime = 0
local timeWaited = 0
local stepTime = 0.1

local randNum

local gripCo

local noteTable = {
	[&quot;f&quot;] = Guitar:FindFirstChild(&quot;C5&quot;),
	[&quot;t&quot;] = Guitar:FindFirstChild(&quot;CSharp5&quot;),
	[&quot;g&quot;] = Guitar:FindFirstChild(&quot;D5&quot;),
	[&quot;y&quot;] = Guitar:FindFirstChild(&quot;EFlat5&quot;),
	[&quot;h&quot;] = Guitar:FindFirstChild(&quot;E5&quot;),
	[&quot;j&quot;] = Guitar:FindFirstChild(&quot;F5&quot;),
	[&quot;n&quot;] = Guitar:FindFirstChild(&quot;FSharp5&quot;),
	[&quot;k&quot;] = Guitar:FindFirstChild(&quot;G5&quot;),
	[&quot;m&quot;] = Guitar:FindFirstChild(&quot;AFlat5&quot;),
	[&quot;l&quot;] = Guitar:FindFirstChild(&quot;A5&quot;),
	[&quot;p&quot;] = Guitar:FindFirstChild(&quot;BFlat5&quot;),
	[&quot;;&quot;] = Guitar:FindFirstChild(&quot;B5&quot;),
	[&quot;&apos;&quot;] = Guitar:FindFirstChild(&quot;C6&quot;)
}

function reformGrip()
	for i = 1, 5 do	-- Game needs to be told quite a few times that we&apos;re serious about getting rid of RightGrip.
		myCharacter = Tool.Parent
		Torso = myCharacter:FindFirstChild(&quot;Torso&quot;)
		RightArm = myCharacter:FindFirstChild(&quot;Right Arm&quot;)
		if RightArm then RightGrip = RightArm:FindFirstChild(&quot;RightGrip&quot;) end

		if Torso and not Torso:FindFirstChild(&quot;Silly Guitar Torso Weld&quot;) then
			if RightGrip then RightGrip.Parent = nil end
			Guitar.CanCollide = false
			TorsoWeld = Instance.new(&quot;Weld&quot;)
			TorsoWeld.Part0 = Torso
			TorsoWeld.Part1 = Guitar
			TorsoWeld.C1 = CFrame.Angles(math.pi/2, math.pi, -math.pi/4) + Vector3.new(-0.2, 0.6, -0.4)
			TorsoWeld.Name = &quot;Silly Guitar Torso Weld&quot;
			TorsoWeld.Parent = game.Workspace
		end
		wait()
	end
end

function onActivated()
	-- Again, make sure character still has arms first.
	myCharacter = Tool.Parent
	RightArm = myCharacter:FindFirstChild(&quot;Right Arm&quot;)
	LeftArm = myCharacter:FindFirstChild(&quot;Left Arm&quot;)
	if not (RightArm and LeftArm) then return end
	-- And that tool is enabled.
	if not Tool.Enabled then return end

	Tool.Enabled = false
	
	randNum = math.random()
	if (randNum &lt; 1/3) then 
		SongSound = Guitar:FindFirstChild(&quot;Song1&quot;)
		waitTime = 11
		stepTime = 0.37
	elseif (randNum &lt; 2/3) then 
		SongSound = Guitar:FindFirstChild(&quot;Song2&quot;)
		waitTime = 9.1
		stepTime = 0.52
	else 
		SongSound = Guitar:FindFirstChild(&quot;Song3&quot;)
		waitTime = 8.1
		stepTime = 0.49
	end

	SongSound:Play()
	timeWaited = 0
	while timeWaited &lt; waitTime do
		if StrumAnim then StrumAnim:Play() end
		wait(stepTime)
		timeWaited = timeWaited + stepTime
	end

	Tool.Enabled = true
end

function play(key)
	--  Make sure character still has arms with which to play.
	myCharacter = Tool.Parent
	RightArm = myCharacter:FindFirstChild(&quot;Right Arm&quot;)
	LeftArm = myCharacter:FindFirstChild(&quot;Left Arm&quot;)
	local Note = noteTable[key:lower()]
	if Note and RightArm and LeftArm then 
		if StrumAnim then StrumAnim:Play() end
		Note:Play() 
	end
end

function onEquipped(mouse)
	if mouse then mouse.KeyDown:connect(play) end

	myCharacter = Tool.Parent
	-- Load animation nao, so we don&apos;t have to do it every time we play a note.
	myHumanoid = myCharacter:FindFirstChild(&quot;Humanoid&quot;)
	if myHumanoid then StrumAnim = myHumanoid:LoadAnimation(Tool.Strum) end
	
	-- And play the animation that gets us in position to strum.
	if myHumanoid then ReadyAnim = myHumanoid:LoadAnimation(Tool.GetGuitarReady) end
	if ReadyAnim then ReadyAnim:Play() end

	-- Break right grip, and weld guitar to torso instead.
	reformGrip()

end

function onUnequipped()
	-- Prepare to wait a while to re-enable if the unequip was via a backspace.
	if (Tool.Parent == game.Workspace and not Tool:FindFirstChild(&quot;RunningWaitScript&quot;)) then
		TimeTillEnabled = Tool:FindFirstChild(&quot;TimeTillEnabled&quot;)
		if TimeTillEnabled then TimeTillEnabled.Value = waitTime - timeWaited end
		WaitScript = Tool:FindFirstChild(&quot;WaitScript&quot;)
		if WaitScript then
			WaitScript = WaitScript:Clone()
			WaitScript.Name = &quot;RunningWaitScript&quot;
			WaitScript.Parent = Tool
			WaitScript.Disabled = false
		end
	end
		
	if ReadyAnim then ReadyAnim:Stop() end
	StrumAnim = nil

	-- Break weld, get scary guitar away from us :O
	TorsoWeld.Parent = nil
	if Torso then
		Guitar.Position = Torso.Position + 8 * Torso.CFrame.lookVector + Vector3.new(0, 5, 0)
	else	
		Guitar.Position = Vector3.new(0, 5, 0)
	end
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)</ProtectedString>
