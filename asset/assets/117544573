end


local CanSpawnCash=true
function SpawnCash()
&#9;CanSpawnCash=false
&#9;--Tool.Grip=CFrame.new(0, 1.39999998, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)
&#9;ShakeAniTrack:Play(.1,1,1)
&#9;--wait(1)
&#9;shakeSound:Play()
