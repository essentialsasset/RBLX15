function GetTotalMass(Parent)
&#9;local Mass = 0
&#9;local function GetMass(Parent)
&#9;&#9;for i, v in pairs(Parent:GetChildren()) do
&#9;&#9;&#9;if v:IsA(&quot;BasePart&quot;) then
&#9;&#9;&#9;&#9;Mass = Mass + v:GetMass()
&#9;&#9;&#9;end
&#9;&#9;&#9;GetMass(v)
&#9;&#9;end
&#9;end
&#9;GetMass(Parent)
&#9;return Mass
end

function IsClose(A, B, Epsilon)
&#9;Epsilon = (Epsilon or 0.01)
&#9;return (A &lt;= (B + Epsilon) and (A &gt;= (B - Epsilon)))
end

function TagHumanoid(humanoid, player)
&#9;local Creator_Tag = Instance.new(&quot;ObjectValue&quot;)
&#9;Creator_Tag.Name = &quot;creator&quot;
&#9;Creator_Tag.Value = player
&#9;Debris:AddItem(Creator_Tag, 2)
&#9;Creator_Tag.Parent = humanoid
end

function UntagHumanoid(humanoid)
&#9;for i, v in pairs(humanoid:GetChildren()) do
&#9;&#9;if v:IsA(&quot;ObjectValue&quot;) and v.Name == &quot;creator&quot; then
&#9;&#9;&#9;v:Destroy()
&#9;&#9;end
&#9;end
end

function FunctionBat()
&#9;while BatClone and BatClone.Parent and BatClone.Parent == Character and Active do
&#9;&#9;if not BatBody.Parent or not LeftShoulder.Parent or not RightShoulder.Parent then
&#9;&#9;&#9;Die()
&#9;&#9;&#9;break
&#9;&#9;end
&#9;&#9;local CameraLookVector = Camera.CoordinateFrame.lookVector
&#9;&#9;BatBody.CFrame = CFrame.new(BatBody.Position, BatBody.Position + CameraLookVector)
&#9;&#9;local ForwardVector = BatBody.CFrame.lookVector
&#9;&#9;local UpVector = BatBody.CFrame:vectorToWorldSpace(Vector3.new(0, 1, 0))
&#9;&#9;local RightVector = ForwardVector:Cross(UpVector)
&#9;&#9;local MoveVector = Vector3.new()
&#9;&#9;if Forward then
&#9;&#9;&#9;MoveVector = (MoveVector + ForwardVector)
&#9;&#9;end
&#9;&#9;if Backward then
&#9;&#9;&#9;MoveVector = (MoveVector - ForwardVector)
&#9;&#9;end
&#9;&#9;if Left then
&#9;&#9;&#9;MoveVector = (MoveVector - RightVector)
&#9;&#9;end
&#9;&#9;if Right then
&#9;&#9;&#9;MoveVector = (MoveVector + RightVector)
&#9;&#9;end
&#9;&#9;if Up then
&#9;&#9;&#9;MoveVector = (MoveVector + UpVector)
&#9;&#9;end
&#9;&#9;if Down then
&#9;&#9;&#9;MoveVector = (MoveVector - UpVector)
&#9;&#9;end
&#9;&#9;TargetPoint = (TargetPoint + (MoveVector * BatMoveFactor))
&#9;&#9;BatBody.CFrame = BatBody.CFrame - BatBody.Position + BatBody.Position:Lerp(TargetPoint, 0.25)
&#9;&#9;if Flapping then
&#9;&#9;&#9;if not Waiting then
&#9;&#9;&#9;&#9;local Angle = ((WingsDown and 0.5) or -0.4)
&#9;&#9;&#9;&#9;LeftShoulder.DesiredAngle = Angle
&#9;&#9;&#9;&#9;RightShoulder.DesiredAngle = Angle
&#9;&#9;&#9;&#9;LastWingAngle = Angle
&#9;&#9;&#9;&#9;Waiting = true
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;if IsClose(LeftShoulder.CurrentAngle, LastWingAngle) or IsClose(RightShoulder.CurrentAngle, LastWingAngle) then
&#9;&#9;&#9;&#9;&#9;WingsDown = not WingsDown
&#9;&#9;&#9;&#9;&#9;Waiting = false
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;&#9;local Now = time()
&#9;&#9;&#9;if (Now - LastRandomSqueakTime) &gt; 1 and math.random() &lt; 0.1 then
&#9;&#9;&#9;&#9;SqueakSounds[math.random(1, #SqueakSounds)]:Play()
&#9;&#9;&#9;&#9;LastRandomSqueakTime = Now
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;wait(OneStep)
&#9;end
end

function BatTouched(Hit)
&#9;if Hit and Hit.Parent and Hit.Parent ~= Character and Hit.Parent ~= BatClone then
&#9;&#9;local humanoid = Hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;if humanoid then
&#9;&#9;&#9;UntagHumanoid(humanoid)
&#9;&#9;&#9;TagHumanoid(humanoid, Player)
&#9;&#9;&#9;humanoid:TakeDamage(TouchDamage)
&#9;&#9;end
&#9;end
end

function MakeTransparent(Parent, Transparency)
&#9;for i, v in pairs(Parent:GetChildren()) do
&#9;&#9;pcall(function()
&#9;&#9;&#9;if v ~= HumanoidRootPart then
&#9;&#9;&#9;&#9;v.Transparency = Transparency
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;&#9;MakeTransparent(v, Transparency)
&#9;end
end

function Transform()
&#9;Tool.CanBeDropped = false
&#9;StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
&#9;if IdleAnimTrack then
&#9;&#9;IdleAnimTrack:Stop()
&#9;end
&#9;MakeTransparent(Character, 1)
&#9;local Creator = Instance.new(&quot;ObjectValue&quot;)
&#9;Creator.Name = &quot;Creator&quot;
&#9;Creator.Value = Player
&#9;local Model = Instance.new(&quot;ObjectValue&quot;)
&#9;Model.Name = &quot;Model&quot;
&#9;BatClone = Bat:Clone()
&#9;Model.Value = BatClone
&#9;local CharacterMonitorClone = CharacterMonitor:Clone()
&#9;Creator.Parent = CharacterMonitorClone
&#9;Model.Parent = CharacterMonitorClone
&#9;CharacterMonitorClone.Disabled = false
&#9;CharacterMonitorClone.Parent = game:GetService(&quot;Workspace&quot;)&#9;
&#9;BatBody = BatClone:FindFirstChild(&quot;Torso&quot;)
&#9;LeftShoulder = BatBody:FindFirstChild(&quot;Left Shoulder&quot;)
&#9;RightShoulder = BatBody:FindFirstChild(&quot;Right Shoulder&quot;)
&#9;WindSqueak = BatBody:WaitForChild(&quot;WindSqueak&quot;)
&#9;SqueakSounds = {
&#9;&#9;BatBody:WaitForChild(&quot;Squeak1&quot;),
&#9;&#9;BatBody:WaitForChild(&quot;Squeak2&quot;),
&#9;&#9;BatBody:WaitForChild(&quot;Squeak3&quot;)
&#9;}
&#9;BatBody.Anchored = true
&#9;for i, v in pairs(BatClone:GetChildren()) do
&#9;&#9;if v:IsA(&quot;BasePart&quot;) then
&#9;&#9;&#9;v.Touched:connect(BatTouched)
&#9;&#9;end
&#9;end
&#9;BatBody.CFrame = Torso.CFrame
&#9;BatBody.Velocity = Vector3.new()
&#9;BatClone.Parent = Character
&#9;Camera.CameraType = Enum.CameraType.Follow
&#9;Camera.CameraSubject = BatBody
&#9;local HeadWelds = {}
&#9;local RightArm = Character:FindFirstChild(&quot;Right Arm&quot;)
&#9;local RightGripClone = nil
&#9;if RightArm then
&#9;&#9;local RightGrip = RightArm:FindFirstChild(&quot;RightGrip&quot;)
&#9;&#9;if RightGrip then
&#9;&#9;&#9;RightGripClone = RightGrip:Clone()
&#9;&#9;&#9;RightGripClone.Part0 = RightGrip.Part0
&#9;&#9;&#9;RightGripClone.Part1 = RightGrip.Part1
&#9;&#9;end
&#9;end
&#9;for i, v in pairs(Character:GetChildren()) do
&#9;&#9;if v:IsA(&quot;Hat&quot;) then
&#9;&#9;&#9;table.insert(Hats, v)
&#9;&#9;&#9;v.Parent = nil
&#9;&#9;end
&#9;end
&#9;for i, v in pairs(Head:GetChildren()) do
&#9;&#9;if v:IsA(&quot;JointInstance&quot;) and v.Name == &quot;HeadWeld&quot; and v.Part0 and v.Part1 then
&#9;&#9;&#9;table.insert(HeadWelds, {Weld = v, Parent = v.Parent, Size = v.Part1.Size, Part0 = v.Part0, Part1 = v.Part1, C0 = v.C0, C1 = v.C1})
&#9;&#9;end
&#9;end
&#9;for i, v in pairs(Torso:GetChildren()) do
&#9;&#9;if v:IsA(&quot;JointInstance&quot;) and v.Part0 and v.Part1 then
&#9;&#9;&#9;local MotorTable = {Motor = v, Parent = v.Parent, Size = v.Part1.Size, Part0 = v.Part0, Part1 = v.Part1, C0 = v.C0, C1 = v.C1}
&#9;&#9;&#9;table.insert(Motors, MotorTable)
&#9;&#9;&#9;v.Part1.FormFactor = Enum.FormFactor.Custom
&#9;&#9;&#9;v.Part1.Size = Vector3.new(0.2, 0.2, 0.2)
&#9;&#9;&#9;local Motor = Instance.new(MotorTable.Motor.ClassName)
&#9;&#9;&#9;Motor.Name = MotorTable.Motor.Name
&#9;&#9;&#9;Motor.C0 = CFrame.new(0, 0, 0)
&#9;&#9;&#9;Motor.C1 = CFrame.new(0, 0, 0)
&#9;&#9;&#9;Motor.Part0 = MotorTable.Part0
&#9;&#9;&#9;Motor.Part1 = MotorTable.Part1
&#9;&#9;&#9;Motor.Parent = MotorTable.Parent
&#9;&#9;end
&#9;end
&#9;for i, v in pairs(HeadWelds) do
&#9;&#9;local Weld = Instance.new(v.Weld.ClassName)
&#9;&#9;Weld.Name = v.Weld.Name
&#9;&#9;Weld.Part0 = v.Part0
&#9;&#9;Weld.Part1 = v.Part1
&#9;&#9;Weld.C0 = v.C0
&#9;&#9;Weld.C1 = v.C1
&#9;&#9;Weld.Parent = v.Parent
&#9;end
&#9;if RightArm and RightGripClone then
&#9;&#9;RightGripClone.Parent = RightArm
&#9;end
&#9;
&#9;local FakeHeadWeld = Instance.new(&quot;Weld&quot;)
&#9;FakeHeadWeld.Name = &quot;HeadWeld&quot;
&#9;FakeHeadWeld.Parent = Head
&#9;
&#9;local BatWeld = Instance.new(&quot;Motor6D&quot;)
&#9;BatWeld.Name = &quot;BatWeld&quot;
&#9;BatWeld.Part0 = BatBody
&#9;BatWeld.Part1 = Torso
&#9;BatWeld.Parent = BatBody

&#9;TargetPoint = BatBody.Position

&#9;local Smoke = Instance.new(&quot;Smoke&quot;)
&#9;Delay(0.3, function()
&#9;&#9;Smoke.Enabled = false
&#9;end)
&#9;Smoke.Parent = BatBody
&#9;Debris:AddItem(Smoke, 5)

&#9;WindSqueak:Play()
&#9;
&#9;Active = true
&#9;
&#9;for i, v in pairs(Character:GetChildren()) do
&#9;&#9;if v and v.Parent and v:IsA(&quot;Tool&quot;) and v ~= Tool then
&#9;&#9;&#9;v.Parent = Backpack
&#9;&#9;end
&#9;end
&#9;
&#9;if ToolAdded then
&#9;&#9;ToolAdded:disconnect()
&#9;end
&#9;ToolAdded = Character.ChildAdded:connect(function(Child)
&#9;&#9;wait(0.25)
&#9;&#9;if Child and Child.Parent and Child:IsA(&quot;Tool&quot;) and Child ~= Tool then
&#9;&#9;&#9;Child.Parent = Backpack
&#9;&#9;&#9;Humanoid:EquipTool(Tool)
&#9;&#9;end
&#9;end)
&#9;
&#9;Spawn(function()
&#9;&#9;FunctionBat()
&#9;end)
&#9;
&#9;Humanoid:EquipTool(Tool)

end

function UnTransform()
&#9;Waiting = false
&#9;SqueakSounds = {}
&#9;StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
&#9;if ToolAdded then
&#9;&#9;ToolAdded:disconnect()
&#9;end
&#9;for i, v in pairs(Directions) do
&#9;&#9;v = false
&#9;end
&#9;if BatClone and BatClone.Parent then
&#9;&#9;BatClone:Destroy()
&#9;end
&#9;for i, v in pairs(Hats) do
&#9;&#9;v.Parent = Character
&#9;end
&#9;Hats = {}
&#9;
&#9;Camera.CameraType = Enum.CameraType.Custom
&#9;Camera.CameraSubject = Humanoid
&#9;local HeadWelds = {}
&#9;local RightArm = Character:FindFirstChild(&quot;Right Arm&quot;)
&#9;local RightGripClone = nil
&#9;if RightArm then
&#9;&#9;local RightGrip = RightArm:FindFirstChild(&quot;RightGrip&quot;)
&#9;&#9;if RightGrip then
&#9;&#9;&#9;RightGripClone = RightGrip:Clone()
&#9;&#9;&#9;RightGripClone.Part0 = RightGrip.Part0
&#9;&#9;&#9;RightGripClone.Part1 = RightGrip.Part1
&#9;&#9;end
&#9;end
&#9;for i, v in pairs(Head:GetChildren()) do
&#9;&#9;if v:IsA(&quot;JointInstance&quot;) and v.Name == &quot;HeadWeld&quot; and v.Part0 and v.Part1 then
&#9;&#9;&#9;table.insert(HeadWelds, {Weld = v, Parent = v.Parent, Size = v.Part1.Size, Part0 = v.Part0, Part1 = v.Part1, C0 = v.C0, C1 = v.C1})
&#9;&#9;end
&#9;end
&#9;for i, v in pairs(Motors) do
&#9;&#9;v.Part1.Size = v.Size
&#9;&#9;local Motor = Instance.new(v.Motor.ClassName)
&#9;&#9;Motor.Name = v.Motor.Name
&#9;&#9;Motor.C0 = v.C0
&#9;&#9;Motor.C1 = v.C1
&#9;&#9;Motor.Part0 = v.Part0
&#9;&#9;Motor.Part1 = v.Part1
&#9;&#9;Motor.Parent = v.Parent
&#9;end
&#9;for i, v in pairs(HeadWelds) do
&#9;&#9;local Weld = Instance.new(v.Weld.ClassName)
&#9;&#9;Weld.Name = v.Weld.Name
&#9;&#9;Weld.Part0 = v.Part0
&#9;&#9;Weld.Part1 = v.Part1
&#9;&#9;Weld.C0 = v.C0
&#9;&#9;Weld.C1 = v.C1
&#9;&#9;Weld.Parent = v.Parent
&#9;end
&#9;if RightArm and RightGripClone then
&#9;&#9;RightGripClone.Parent = RightArm
&#9;end
&#9;MotorTable = {}
&#9;MakeTransparent(Character, 0)
&#9;if IdleAnimTrack then
&#9;&#9;IdleAnimTrack:Play()
&#9;end
&#9;Motors = {}&#9;
&#9;Torso.Velocity = Vector3.new()
&#9;local Smoke = Instance.new(&quot;Smoke&quot;)
&#9;Delay(0.3, function()
&#9;&#9;Smoke.Enabled = false
&#9;end)
&#9;Smoke.Parent = BatBody
&#9;Debris:AddItem(Smoke, 5)
&#9;WindBreath:Play()
&#9;local HandlePieces = Handle:GetChildren()
&#9;table.insert(HandlePieces, Handle)
&#9;for i, v in pairs(HandlePieces) do
&#9;&#9;if v:IsA(&quot;BasePart&quot;) then
&#9;&#9;&#9;v.Transparency = 1
&#9;&#9;end
&#9;end
&#9;Active = false
&#9;Tool.CanBeDropped = true
end

function Die()
&#9;if not Died then
&#9;&#9;Died = true
&#9;&#9;StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
&#9;&#9;for i, v in pairs(BatClone:GetChildren()) do
&#9;&#9;&#9;if v:IsA(&quot;BasePart&quot;) then
&#9;&#9;&#9;&#9;v.Name = &quot;Part&quot;
&#9;&#9;&#9;&#9;v.CanCollide = false
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;for i, v in pairs(Character:GetChildren()) do
&#9;&#9;&#9;if v:IsA(&quot;BasePart&quot;) then
&#9;&#9;&#9;&#9;v:Destroy()
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;if Humanoid and Humanoid.Health &gt; 0 then
&#9;&#9;&#9;Humanoid.Health = 0
&#9;&#9;end
&#9;&#9;if BatClone.Parent == Character then
&#9;&#9;&#9;for i, v in pairs(BatBody:GetChildren()) do
&#9;&#9;&#9;&#9;if v:IsA(&quot;JointInstance&quot;) then
&#9;&#9;&#9;&#9;&#9;v:Destroy()
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;&#9;BatBody.Anchored = false
&#9;&#9;end
&#9;&#9;Debris:AddItem(BatClone, 2)
&#9;&#9;Debris:AddItem(CloakClone, 2)
&#9;end
end

function OnKey(Key, IsDown)
&#9;if not Player or not Backpack or not Humanoid or Humanoid.Health == 0 or not Head or not HumanoidRootPart or not Torso then
&#9;&#9;return
&#9;end
&#9;Key = Key:lower()
&#9;if Key == &quot;w&quot; or Key == &quot;&#17;&quot; then
&#9;&#9;Forward = IsDown
&#9;elseif Key == &quot;s&quot; or Key == &quot;&#18;&quot; then
&#9;&#9;Backward = IsDown
&#9;elseif Key == &quot;a&quot; or Key == &quot;&#20;&quot; then
&#9;&#9;Left = IsDown
&#9;elseif Key == &quot;d&quot; or Key == &quot;&#19;&quot; then
&#9;&#9;Right = IsDown
&#9;elseif Key == &quot;e&quot; then
&#9;&#9;Up = IsDown
&#9;elseif Key == &quot;q&quot; then
&#9;&#9;Down = IsDown
&#9;elseif Key == &quot; &quot; and IsDown then
&#9;&#9;if BatClone.Parent == Character then
&#9;&#9;&#9;UnTransform()
&#9;&#9;elseif Equipped and not Active then
&#9;&#9;&#9;if (tick() - JumpTick) &lt;= JumpTime or JumpTick == 0 and Character.Parent then
&#9;&#9;&#9;&#9;JumpTick = tick()
&#9;&#9;&#9;&#9;Jumps = Jumps + 1
&#9;&#9;&#9;&#9;if Jumps &gt;= JumpsRequired then
&#9;&#9;&#9;&#9;&#9;JumpTick = 0
&#9;&#9;&#9;&#9;&#9;Jumps = 0
&#9;&#9;&#9;&#9;&#9;Transform()
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;JumpTick = tick()
&#9;&#9;&#9;&#9;Jumps = 1
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
end

function Activated()
&#9;if BatClone.Parent ~= Character and (not CloakAntiGravity or not CloakAntiGravity.Parent) then
&#9;&#9;Base:Play()
&#9;&#9;CloakAntiGravity = Instance.new(&quot;BodyForce&quot;)
&#9;&#9;CloakAntiGravity.Name = &quot;CloakAntiGravity&quot;
&#9;&#9;CloakAntiGravity.force = Vector3.new(0, (GetTotalMass(Character) * Gravity), 0)
&#9;&#9;local ChildAddedConnection = Character.ChildRemoved:connect(function()
&#9;&#9;&#9;CloakAntiGravity.force = Vector3.new(0, (GetTotalMass(Character) * Gravity), 0)
&#9;&#9;end)
&#9;&#9;local ChildRemovedConnection = Character.ChildAdded:connect(function()
&#9;&#9;&#9;CloakAntiGravity.force = Vector3.new(0, (GetTotalMass(Character) * Gravity), 0)
&#9;&#9;end)
&#9;&#9;CloakAntiGravity.Parent = Torso
&#9;&#9;Debris:AddItem(CloakAntiGravity, CloakFloatTime)
&#9;&#9;Delay(CloakFloatTime, function()
&#9;&#9;&#9;if ChildAddedConnection then
&#9;&#9;&#9;&#9;ChildAddedConnection:disconnect()
&#9;&#9;&#9;end
&#9;&#9;&#9;if ChildRemovedConnection then
&#9;&#9;&#9;&#9;ChildRemovedConnection:disconnect()
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;end
end

function Equipped()
&#9;if Active then
&#9;&#9;return
&#9;end
&#9;Character = Tool.Parent
&#9;Player = Players:GetPlayerFromCharacter(Character)
&#9;Backpack = Player:FindFirstChild(&quot;Backpack&quot;)
&#9;Humanoid = Character:FindFirstChild(&quot;Humanoid&quot;)
&#9;Head = Character:FindFirstChild(&quot;Head&quot;)
&#9;Torso = Character:FindFirstChild(&quot;Torso&quot;)
&#9;HumanoidRootPart = Character:FindFirstChild(&quot;HumanoidRootPart&quot;)
&#9;if not Player or not Backpack or not Humanoid or Humanoid.Health == 0 or not Head or not HumanoidRootPart or not Torso then
&#9;&#9;return
&#9;end
&#9;Mouse = Player:GetMouse()
&#9;local HandlePieces = Handle:GetChildren()
&#9;table.insert(HandlePieces, Handle)
&#9;for i, v in pairs(HandlePieces) do
&#9;&#9;if v:IsA(&quot;BasePart&quot;) then
&#9;&#9;&#9;v.Transparency = 1
&#9;&#9;end
&#9;end
&#9;Spawn(function()
&#9;&#9;CloakClone = Cloak:Clone()
&#9;&#9;local Weld = Instance.new(&quot;Weld&quot;)
&#9;&#9;Weld.Name = &quot;CloakWeld&quot;
&#9;&#9;Weld.Part0 = Torso
&#9;&#9;Weld.Part1 = CloakClone
&#9;&#9;Weld.C1 = CFrame.new(0, 0.545642853, -0.638683319)
&#9;&#9;Weld.Parent = Torso
&#9;&#9;CloakClone.Parent = Character
&#9;end)
&#9;IdleAnim = Humanoid:LoadAnimation(Idle)
&#9;if IdleAnim then
&#9;&#9;IdleAnim:Play()
&#9;end
&#9;Humanoid.WalkSpeed = 22
&#9;if DiedConnection then
&#9;&#9;DiedConnection:disconnect()
&#9;end
&#9;DiedConnection = Humanoid.Died:connect(Die)
&#9;if not KeyDown then
&#9;&#9;KeyDown = Mouse.KeyDown:connect(function(Key)
&#9;&#9;&#9;OnKey(Key, true)
&#9;&#9;end)
&#9;end
&#9;if not KeyUp then
&#9;&#9;KeyUp = Mouse.KeyUp:connect(function(Key)
&#9;&#9;&#9;OnKey(Key, false)
&#9;&#9;end)
&#9;end
&#9;Equipped = true
end

function Unequipped()
&#9;if Active then
&#9;&#9;return
&#9;end
&#9;Equipped = false
&#9;Humanoid.WalkSpeed = 16
&#9;if CloakAntiGravity and CloakAntiGravity.Parent then
&#9;&#9;CloakAntiGravity:Destroy()
&#9;end
&#9;if CloakClone and CloakClone.Parent then
&#9;&#9;CloakClone:Destroy()
&#9;end
&#9;if IdleAnim then
&#9;&#9;IdleAnim:Stop()
&#9;end
&#9;if Active then return end
&#9;local RightArm = Character:FindFirstChild(&quot;Right Arm&quot;)
&#9;if RightArm then
&#9;&#9;for i, v in pairs(RightArm:GetChildren()) do
&#9;&#9;&#9;if v:IsA(&quot;JointInstance&quot;) and v.Name == &quot;RightGrip&quot; then
&#9;&#9;&#9;&#9;v:Destroy()
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;local HandlePieces = Handle:GetChildren()
&#9;table.insert(HandlePieces, Handle)
&#9;for i, v in pairs(HandlePieces) do
&#9;&#9;if v:IsA(&quot;BasePart&quot;) then
&#9;&#9;&#9;v.Transparency = 0
&#9;&#9;end
&#9;end
end

