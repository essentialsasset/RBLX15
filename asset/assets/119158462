function getNearbyHumanoids()
&#9;local stuff = game.Workspace:GetChildren()
&#9;local humanoids = {}
&#9;for i = 1,#stuff do
&#9;&#9;if stuff[i].Name ~= &quot;Stalker&quot; and stuff[i].Name ~= &quot;Ragdoll&quot; then
&#9;&#9;&#9;if stuff[i]:findFirstChild(&quot;Torso&quot;) then
&#9;&#9;&#9;&#9;if stuff[i]:findFirstChild(&quot;Humanoid&quot;) then
&#9;&#9;&#9;&#9;&#9;if stuff[i] ~= script.Parent then
&#9;&#9;&#9;&#9;&#9;&#9;if (stuff[i].Torso.Position-script.Parent.Torso.Position).magnitude &lt; 20 then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;table.insert(humanoids,stuff[i].Humanoid)
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;return humanoids
end

