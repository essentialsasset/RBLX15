&#9;&#9;&#9;elseif Key == &quot;s&quot; then
&#9;&#9;&#9;&#9;Back = true
&#9;&#9;&#9;&#9;while Back and equipped do
&#9;&#9;&#9;&#9;&#9;if CurrentSpeed &gt; 0 then
&#9;&#9;&#9;&#9;&#9;&#9;CurrentSpeed = math.max(-20, (CurrentSpeed - (Deceleration * 2.8 * (1 / 30))))
&#9;&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;&#9;CurrentSpeed = math.max(-20, (CurrentSpeed - (Deceleration * (1 / 30))))
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;wait(1 / 30)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;elseif Key == &quot;d&quot; then
&#9;&#9;&#9;&#9;Right = true
&#9;&#9;&#9;&#9;while Right and equipped do
&#9;&#9;&#9;&#9;&#9;TurnSpeed = math.max(-5, (TurnSpeed - (TurnAlpha)))
&#9;&#9;&#9;&#9;&#9;wait(1 / 30)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;elseif Key == &quot; &quot; then
&#9;&#9;&#9;&#9;if (LastSpace == 0 or (tick() - LastSpace) &gt; 1.9) then
&#9;&#9;&#9;&#9;&#9;LastSpace = tick()
&#9;&#9;&#9;&#9;&#9;local BodyForce = Instance.new(&quot;BodyForce&quot;)
&#9;&#9;&#9;&#9;&#9;BodyForce.force = Vector3.new(0, (((GetTotalMass(Character) * 196.20) * 4) * 4), 0)
&#9;&#9;&#9;&#9;&#9;BodyForce.Parent = FakeHandle
