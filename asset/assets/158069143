&#9;&#9;for i, v in pairs(Values) do
&#9;&#9;&#9;if string.find(v, &quot;=&quot;) then
&#9;&#9;&#9;&#9;local Variable = string.sub(v, 1, (string.find(v, &quot;=&quot;) - 1))
&#9;&#9;&#9;&#9;local Value = string.sub(v, (string.find(v, &quot;=&quot;) + 1), string.len(v))
&#9;&#9;&#9;&#9;Variables[Variable] = Value
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;local Sound = Handle:FindFirstChild(Variables.Name)
&#9;&#9;if Sound then
&#9;&#9;&#9;if string.lower(Variables.IsPlaying) == string.lower(&quot;true&quot;) then
&#9;&#9;&#9;&#9;Sound:Play()
&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;Sound:Stop()
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;SoundInput.Value = &quot;&quot;
&#9;end
end)</ProtectedString>
			</Properties>
		</Item>
