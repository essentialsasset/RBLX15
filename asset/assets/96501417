				<ProtectedString name="Source">local function WaitForChild(parent, childName)
&#9;while not parent:FindFirstChild(childName) do parent.ChildAdded:wait() end
&#9;return parent[childName]
end

local Tool = script.Parent;
local Handle = WaitForChild(Tool, &apos;Handle&apos;)
local VINE_DAMAGE = 25
local humanoid = nil
enabled = true
local debris = game:GetService(&quot;Debris&quot;)

local sparkles = WaitForChild(Handle, &apos;Sparkles&apos;)

local bodyRotation = Instance.new(&quot;BodyAngularVelocity&quot;)
bodyRotation.P = 150000
bodyRotation.angularvelocity = Vector3.new(0,25,0)
bodyRotation.maxTorque = Vector3.new(bodyRotation.P,bodyRotation.P,bodyRotation.P)

local RootSample = game:GetService(&quot;InsertService&quot;):LoadAsset(30569222)


local HitHumanoids = {}

function createVine(pos)

&#9;--This call will cause a &quot;wait&quot; until the data comes back
&#9;local root = RootSample:Clone()

&#9;local instances = root:GetChildren()
&#9;if #instances == 0 then
&#9;&#9;root:Remove()
&#9;&#9;return
&#9;end

&#9;--Continue the insert process
&#9;local vineSwirl = root:FindFirstChild(&quot;vine swirl&quot;)
&#9;vineSwirl.Parent = game.Workspace
&#9;vineSwirl:MoveTo(pos)

&#9;for _, vine in pairs(vineSwirl:GetChildren()) do
&#9;&#9;if vine:FindFirstChild(&apos;Mesh&apos;) then
&#9;&#9;&#9;vine.Mesh.VertexColor = Vector3.new(1, math.random() / 2, 0)
&#9;&#9;end
&#9;end

&#9;debris:AddItem(vineSwirl, 12)
&#9;Delay(12, function()
&#9;&#9;sparkles.Enabled = false
&#9;end)
&#9;return vineSwirl
end

function fire(v)

&#9;
&#9;local vCharacter = Tool.Parent
&#9;local vPlayer = game.Players:playerFromCharacter(vCharacter)

&#9;local numOfVines = 12
&#9;
&#9;local increment = (math.pi *2)/numOfVines

&#9;torsoNormal = humanoid.Parent.Torso.CFrame.lookVector
&#9;denom = math.abs(torsoNormal.x) + math.abs(torsoNormal.z)
&#9;posX = 15 * (torsoNormal.x/denom)
&#9;posZ = 15 * (torsoNormal.z/denom)
&#9;local pos = Vector3.new(v.x + posX,v.y, v.z + posZ)
&#9;local vines = {}

&#9;for i = 1, numOfVines do
 
&#9;&#9;local angle = increment * i
&#9;&#9;table.insert(vines,createVine(pos))
&#9;&#9;pos = Vector3.new(((pos.x - v.x) * math.cos(angle)) - ((pos.z - v.z) * math.sin(angle)) + v.x,
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9; vCharacter.Torso.Position.y,
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;((pos.x - v.x) * math.sin(angle)) + ((pos.z - v.z) * math.cos(angle)) + v.z)

&#9;end

&#9;scriptIt(vines)

end


function scriptIt(vines)

&#9;for i = 1, #vines do
&#9;&#9;local parts = vines[i]:GetChildren()
&#9;&#9;for j = 1, #parts do
&#9;&#9;&#9;parts[j].Touched:connect(touched)
&#9;&#9;end
&#9;end

end

function touched(part)
&#9;if part and part.Parent and part.Parent:FindFirstChild(&apos;Humanoid&apos;) and not HitHumanoids[part.Parent.Humanoid] then
&#9;&#9;local humanoid = part.Parent.Humanoid
&#9;&#9;HitHumanoids[humanoid] = tick()
&#9;&#9;humanoid:TakeDamage(VINE_DAMAGE)
&#9;&#9;wait(1)
&#9;&#9;HitHumanoids[humanoid] = nil
&#9;end
end


function staffUp()
&#9;Tool.GripForward = Vector3.new(0,0,-1)
&#9;Tool.GripRight = Vector3.new(1,0,0)
&#9;Tool.GripUp = Vector3.new(0,1,0)
end

function staffOut()
&#9;Tool.GripForward = Vector3.new(.0976, .00976, -.995)
&#9;Tool.GripRight = Vector3.new(.195, -.981, .00952)
&#9;Tool.GripUp = Vector3.new(.976, .195, .0976)
end


function onActivated()
&#9;if not enabled  then
&#9;&#9;return
&#9;end

&#9;enabled = false
&#9;HitHumanoids = {}
&#9;local character = Tool.Parent;
&#9;humanoid = character.Humanoid
&#9;if humanoid == nil then
&#9;&#9;print(&quot;Humanoid not found&quot;)
&#9;&#9;return 
&#9;end

&#9;local targetPos = humanoid.TargetPoint
&#9;local lookAt = (targetPos - character.Head.Position).unit

&#9;local reload = 15

&#9;staffOut()
&#9;sparkles.Enabled = true
&#9;Tool.Handle.PlantSound:Play()
&#9;debris:AddItem(bodyRotation, 5)
&#9;bodyRotation.Parent = character.Torso
&#9;fire(character.Torso.Position)
&#9;wait(.5)
&#9;staffUp()
&#9;bodyRotation.Parent = nil
&#9;wait(reload)

&#9;enabled = true

end

function onEquipped()
&#9;Tool.Handle.EquipSound:play()
end

function onUnequipped()
 sparkles.Enabled = false
end

script.Parent.Unequipped:connect(onUnequipped)
script.Parent.Activated:connect(onActivated)
script.Parent.Equipped:connect(onEquipped)
