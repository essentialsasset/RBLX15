&#9;guitarAnim = myHumanoid:LoadAnimation(Tool.GuitarAnim)
&#9;if guitarAnim then guitarAnim:Play() end&#9;
&#9;tick() -- random seeding &#9;&#9;
&#9;playSound = Tool:FindFirstChild(&quot;PlaySound&quot;)
&#9;if playSound then playSound.Value = math.random(1, 3) end
&#9;wait(10)
&#9;if playSound then playSound.Value = 0 end
&#9;wait(1.0)
&#9;Guitar.Transparency = 0.0
&#9;if guitarClone then guitarClone:Remove() end
&#9;Tool.Enabled = true&#9;
&#9;
end

function onEquipped(mouse)
&#9;vCharacter = Tool.Parent
&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;if mouse then 
&#9;&#9;mouse.KeyDown:connect(stunThemAll)
