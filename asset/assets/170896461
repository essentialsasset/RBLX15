&#9;for i=1, 65 do
&#9;&#9;local e = gearConfig.gear.Effect:Clone();
&#9;&#9;e.CFrame = gearConfig.gear.Effect.CFrame;
&#9;&#9;e.Anchored = true;
&#9;&#9;e.CanCollide = false;
